/* ************************************************************************** */

#include "../../includes/push_swap.h"

int	ft_lstsize(t_list *lst)
{
	size_t		i;
	t_list		*first;

	i = 0;
	first = lst;
	while (lst != NULL)
	{
		lst->index = i;
		lst = lst->next;
		i++;
	}
	return (i);
}
