#include "../pipex.h"

char	*ft_strdup(const char *s1)
{
	char	*result;
	size_t	to_allocate;

	to_allocate = ft_strlen(s1) + 1;
	result = (char *)malloc(to_allocate);
	if (!result)
		return (NULL);
	ft_strlcpy(result, s1, to_allocate);
	return (result);
}
